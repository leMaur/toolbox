{
    "name": "lemaur/toolbox",
    "description": "This is my package Toolbox",
    "keywords": [
        "lemaur",
        "laravel",
        "toolbox"
    ],
    "homepage": "https://github.com/lemaur/toolbox",
    "license": "MIT",
    "authors": [
        {
            "name": "Maurizio",
            "email": "hello@lemaur.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Lemaur\\Toolbox\\": "src",
            "Lemaur\\Toolbox\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lemaur\\Toolbox\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lemaur\\Toolbox\\ToolboxServiceProvider"
            ],
            "aliases": {
                "Toolbox": "Lemaur\\Toolbox\\ToolboxFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
